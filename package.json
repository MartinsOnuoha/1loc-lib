{
  "name": "1loc",
  "version": "1.1.2",
  "description": "One line of code as a library",
  "main": "dist/src/index.js",
  "private": false,
  "repository": "https://github.com/MartinsOnuoha/1loc-lib",
  "contributors": [
    {
      "name": "Uchenna Jeremiah",
      "email": "ucejtech@gmail.com",
      "url": "https://github.com/ucejtech"
    },
    {
      "name": "Martins Onuoha",
      "email": "martinsvictor.nd@gmail.com",
      "url": "https://github.com/MartinsOnuoha"
    }
  ],
  "types": "./src/index.ts",
  "scripts": {
    "test": "jest --verbose",
    "watch:test": "jest --verbose --watch",
    "watch:build": "no -p tsconfig.json -w",
    "build:main": "tsc -p tsconfig.json",
    "build": "ts-node -r tsconfig-paths/register ./index.ts",
    "build:publish": "rm ./*.tgz && yarn build:main && npm pack && npm publish",
    "commit": "git add . && git cz",
    "run:example": "node ./examples"
  },
  "keywords": [
    "1loc"
  ],
  "engines": {
    "node": ">=14"
  },
  "author": "MartinsOnuoha",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/dom": "^8.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/jest": "^27.0.1",
    "babel-jest": "^27.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.3.1",
    "jest": "^27.1.0",
    "jest-module-name-mapper": "^0.1.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "_moduleAliases": {
    "@array": "dist/groups/array",
    "@dom": "dist/groups/dom"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
