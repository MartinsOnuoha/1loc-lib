{
  "name": "1loc",
  "version": "1.0.1",
  "description": "One line of code as a library",
  "main": "dist/src/index.js",
  "private": false,
  "repository": "https://github.com/MartinsOnuoha/1loc-lib",
  "types": "./src/index.ts",
  "scripts": {
    "test": "jest --verbose",
    "watch:test": "jest --verbose --watch",
    "watch:build": "tsc -p tsconfig.json -w",
    "build:main": "tsc -p tsconfig.json",
    "commit": "git cz"
  },
  "keywords": [
    "1loc"
  ],
  "files": [
    "dist/src",
    "dist",
    "defaultConfig",
    "defaultJestConfig",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/*.config.*",
    "!**/*.json"
  ],
  "engines": {
    "node": ">=14"
  },
  "author": "MartinsOnuoha",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "babel-jest": "^27.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.3.1",
    "jest": "^27.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  }
}
